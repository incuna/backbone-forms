{
  "name": "backbone-forms-zepto",
  "description": "Form framework for BackboneJS with nested forms, editable lists and validation. Packaged for jamjs with zepto dependency",
  "author": {
    "name": "Charles Davison"
  },
  "version": "0.10.0",
  "dependencies": {
    "backbone": ">=0.9.2"
  },
  "devDependencies": {
    "buildify": "latest",
    "underscore": "latest",
    "uglify-js": "latest"
  },
  "main": "src/backbone-forms.js",
  "jam": {
    "dependencies": {
      "backbone-zepto": ">=0.9.2",
      "zepto": "*"
    },
    "main": "distribution/backbone-forms.amd.js",
    "include": ["distribution"]
  }
}
